house_monitor : interceptor {
    NODE house_thermo.house
    PUBLISHES
        house_report { TOPIC house_health }
    CXX { HEADER   "house_p.h"
          FILENAME "house_q.cpp"
          CLASS "HouseQ" }

}

house_health : topic {
    FIELDS
        flag : uint8 0
}

extra : node {
    SUBSCRIBES
        heater_rate { TOPIC house_thermo.heater_data MAXLATENCY 1msec}
    PERIOD 20msec
}
