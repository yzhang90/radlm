  SUBSCRIBES
# ocu -> landshark
    i1 { TOPIC ocu.t__controller_select MAXLATENCY global.radio_latency }
    i2 { TOPIC ocu.t__map MAXLATENCY global.radio_latency }
    i3 { TOPIC ocu.t__goal MAXLATENCY global.radio_latency }
    i4 { TOPIC ocu.t__ccc_reference_speed MAXLATENCY global.radio_latency }
# landshark -> ocu
    i5 { TOPIC topics.t__controller_status MAXLATENCY global.radio_latency }
    i6 { TOPIC topics.t__base_status MAXLATENCY global.radio_latency }
    i7 { TOPIC topics.t__base MAXLATENCY global.radio_latency }
    i8 { TOPIC topics.t__base_actuator_input MAXLATENCY global.radio_latency }
    i9 { TOPIC topics.t__monitor_estop MAXLATENCY global.radio_latency }
  PUBLISHES
# ocu -> landshark
    o1 { TOPIC topics.t__controller_select }
    o2 { TOPIC topics.t__map }
    o3 { TOPIC topics.t__goal }
    o4 { TOPIC topics.t__ccc_reference_speed }
# landshark -> ocu
    o5 { TOPIC ocu.t__controller_status }
    o6 { TOPIC ocu.t__base_status }
    o7 { TOPIC ocu.t__base }
    o8 { TOPIC ocu.t__base_actuator_input }
    o9 { TOPIC ocu.t__monitor_estop }
  PERIOD gateway_gui_period
  CXX { PATH "src/gateway" HEADER "gateway_9.h" CLASS "identity_9" }
}

