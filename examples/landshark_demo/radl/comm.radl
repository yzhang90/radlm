gateway_joystick_period: duration 20msec
gateway_gui_period: duration 50msec

gateway_teleop : node {
  SUBSCRIBES
    i1 { TOPIC ocu.t__teleop_base MAXLATENCY global.radio_latency }
    i2 { TOPIC ocu.t__teleop_estop MAXLATENCY global.radio_latency }
    i3 { TOPIC ocu.t__teleop_over_ride MAXLATENCY global.radio_latency }
    i4 { TOPIC ocu.t__teleop_turret_pan MAXLATENCY global.radio_latency }
    i5 { TOPIC ocu.t__teleop_turret_tilt MAXLATENCY global.radio_latency }
    i6 { TOPIC ocu.t__teleop_moog_pan MAXLATENCY global.radio_latency }
    i7 { TOPIC ocu.t__teleop_moog_tilt MAXLATENCY global.radio_latency }
    i8 { TOPIC ocu.t__teleop_moog_zoom MAXLATENCY global.radio_latency }
    i9 { TOPIC ocu.t__teleop_paintball_trigger MAXLATENCY global.radio_latency }
  PUBLISHES
    o1 { TOPIC topics.t__teleop_base }
    o2 { TOPIC topics.t__teleop_estop }
    o3 { TOPIC topics.t__teleop_over_ride }
    o4 { TOPIC topics.t__teleop_turret_pan }
    o5 { TOPIC topics.t__teleop_turret_tilt }
    o6 { TOPIC topics.t__teleop_moog_pan }
    o7 { TOPIC topics.t__teleop_moog_tilt }
    o8 { TOPIC topics.t__teleop_moog_zoom }
    o9 { TOPIC topics.t__teleop_paintball_trigger }
  PERIOD gateway_joystick_period
  CXX { PATH "src/gateway" HEADER "gateway_9.h" CLASS "identity_9" }
}

