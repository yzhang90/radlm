n1 : node {
  PUBLISHES o1 {TOPIC t }
  PERIOD 10msec
  CXX {HEADER "src/f1.h" CLASS "N1"}
}

t : topic { FIELDS
  x1 : int32 42
  timestamp = radl.start_time
}

n2 : node {
  SUBSCRIBES i2 {TOPIC t MAXLATENCY 1msec }
  PERIOD 5msec
  CXX {HEADER "src/f2.h" CLASS "N2"}
}

plant : plant { MACHINES { OS { NODES_UID 1000 NODES n1 n2 IMG ""} } }