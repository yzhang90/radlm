
cmake_minimum_required(VERSION 2.8.3)
project(cmu_ros_pub_sub)


find_package(catkin REQUIRED COMPONENTS 
 geometry_msgs 
 landshark_common 
 landshark_gps 
 landshark_msgs 
 msg_auth 
 nav_msgs 
 roscpp 
 sensor_msgs 
 std_msgs 
 tf)


catkin_package(
  INCLUDE_DIRS include
  LIBRARIES ${PROJECT_NAME}
  CATKIN_DEPENDS 
  geometry_msgs 
  landshark_common 
  landshark_gps 
  landshark_msgs 
  msg_auth 
  nav_msgs 
  roscpp 
  sensor_msgs 
  std_msgs 
  tf
)


include_directories(
  ${catkin_INCLUDE_DIRS} include
)


add_library(${PROJECT_NAME} SHARED src/getRosParameters.cpp)
target_link_libraries(${PROJECT_NAME} proj)


add_executable(${PROJECT_NAME}_node src/testRosParameters.cpp)


target_link_libraries(${PROJECT_NAME}_node ${PROJECT_NAME} ${catkin_LIBRARIES})

