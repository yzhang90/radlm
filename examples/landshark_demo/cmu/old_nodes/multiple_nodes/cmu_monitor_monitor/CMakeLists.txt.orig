cmake_minimum_required(VERSION 2.8.3)
project(cmu_monitor_monitor)

find_package(
 catkin REQUIRED COMPONENTS 
 geometry_msgs
	cmu_ros_pub_sub
	cmu_sensor_monitor
 landshark_common 
 landshark_gps
	landshark_msgs
 nav_msgs
 tf
 visualization_msgs
 roscpp
 std_msgs
 message_generation
 sensor_msgs
)

add_message_files(
	DIRECTORY msg
	FILES
##	cmu_velocity.msg
##	cmu_deadman.msg
)

include_directories(
 ${catkin_INCLUDE_DIRS}
)

generate_messages(
 DEPENDENCIES
 std_msgs
 geometry_msgs
 landshark_msgs
)

catkin_package(
 CATKIN_DEPENDS
	geometry_msgs   
	landshark_common 
	landshark_gps 
	landshark_msgs 
	nav_msgs 
	roscpp 
	sensor_msgs 
	tf 
	visualization_msgs 
	message_runtime
 std_msgs 
)

add_executable(cmu_monitor_monitor src/cmu_monitor_monitor.cpp)
target_link_libraries(cmu_monitor_monitor ${catkin_LIBRARIES})

