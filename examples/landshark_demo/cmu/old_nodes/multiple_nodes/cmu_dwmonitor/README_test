

roslaunch landshark_cmu_dwmonitor dwmonitor.launch
rostopic pub /landshark/dwmonitor/switch landshark_cmu_dwmonitor/dwMonitorSwitch "dwmonitor_switch: 'on'"
rostopic pub /landshark/dwmonitor/obstacle landshark_cmu_dwmonitor/obstacleMsg "type: 1
x: -14.3346282956
y: -22.0576621948
z: 0.0"
rostopic echo /landshark/dwmonitor/obstacle 
rostopic echo /landshark/dwmonitor/status
rostopic echo /landshark/dwmonitor/safeSpeed

rostopic echo /landshark/odom
rosbag play ~/Downloads/waypoint_sri_8_30_2013.bag




