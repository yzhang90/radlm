<?xml version="1.0"?>
<launch>

    <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform_node" respawn="true">

      <param name="magnetic_declination_radians" value="0"/>

      <param name="roll_offset" value="0"/>

      <param name="pitch_offset" value="0"/>

      <param name="yaw_offset" value="0"/>

      <param name="zero_altitude" value="false"/>
<!--      <remap from="imu/data" to="landshark/imu_stub" />-->
      <remap from="imu/data" to="landshark/imu" />
      <remap from="gps/fix" to="landshark/gps" />

      <remap from="odometry/gps" to="initialpose" /> 

    </node>

</launch>
